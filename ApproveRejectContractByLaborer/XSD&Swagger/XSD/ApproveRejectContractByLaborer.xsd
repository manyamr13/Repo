<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://Qiwa/ContractManagement/ApproveRejectContractByLaborer/v1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:bons0="http://Qiwa/ContractManagement/ApproveRejectContractByLaborer/v1.0" xmlns:ibmSchExtn="http://www.ibm.com/schema/extensions" xmlns:Q1="http://Qiwa/Common/Types">

	<xsd:import namespace="http://Qiwa/Common/Types" schemaLocation="../common/types/ESB_common_types_v1.0.xsd"/>


	<xsd:element name="ApproveRejectContractByLaborerRq" type="bons0:ApproveRejectContractByLaborerRq_Type"/>
	<xsd:element name="ApproveRejectContractByLaborerRs" type="bons0:ApproveRejectContractByLaborerRs_Type"/>

	<complexType ibmSchExtn:docRoot="true" name="ApproveRejectContractByLaborerRq_Type">
